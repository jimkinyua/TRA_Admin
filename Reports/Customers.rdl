  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Revenue</DataSourceName>
        <CommandText>SELECT        c.CustomerName, c.ContactPerson, COALESCE (c.Telephone1, c.Telephone2, c.Mobile1, c.Mobile2) AS Contact, c.Email, c.IDNO, sc.SubCountyName, w.WardName,convert(date,c.CreatedDate) RegisteredDate
FROM            Customer c
left join SubCounty sc on c.SubCounty=sc.SubCountyID
left join Wards w on c.Ward=w.WardID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
