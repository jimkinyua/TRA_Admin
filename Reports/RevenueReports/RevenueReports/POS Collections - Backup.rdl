  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="UserID">
          <DataField>UserID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserNames">
          <DataField>UserNames</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MarketID">
          <DataField>MarketID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MarketName">
          <DataField>MarketName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DeviceSerialNo">
          <DataField>DeviceSerialNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>RevenueData</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        il.CreatedBy AS UserID, ag.FirstName + ' ' + ag.MiddleName + ' ' + ag.LastName AS UserNames, ud.MarketID, mk.MarketName, ud.DeviceSerialNo, SUM(il.Amount) AS Amount
FROM            InvoiceLines AS il INNER JOIN
                         ConsolidateInvoice AS ci ON ci.invoicelineId = il.InvoiceLineID INNER JOIN
                         Agents AS ag ON il.CreatedBy = ag.AgentID INNER JOIN
                         UserDevices AS ud ON ud.DeviceUserID = ag.AgentID INNER JOIN
                         Markets AS mk ON ud.MarketID = mk.MarketID
GROUP BY il.CreatedBy, mk.MarketName, ag.FirstName + ' ' + ag.MiddleName + ' ' + ag.LastName, ud.MarketID, ud.DeviceSerialNo</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
